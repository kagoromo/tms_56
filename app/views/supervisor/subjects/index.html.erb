<%= provide :title, t("subject.all_subjects") %>
<div class="row">
  <div class="col-md-8 col-md-offset-2">
    <div class="panel panel-default">
      <div class="panel-heading text-center">
      <h3><%= t "subject.all_subjects" %></h3>
    </div>
      <div class="panel-body">
        <div class="form-group">
          <%= link_to t("subject.new_subject"),
            new_supervisor_subject_path, class: "btn btn-primary col-md-4
            col-md-offset-4" %>
        </div>
        <div class="row">
          <div class="col-md-12">
            <%= render @subjects %>
            <div class="text-center">
              <%= paginate @subjects %>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
